import fastf1
import pandas as np

session = fastf1.get_session(2025, 'Silverstone', 'Qualifying')
session.load(telemetry=True)

fast_lap = session.laps.pick_driver("VER").pick_fastest()

telem_data = fast_lap.get_telemetry()

nec_data = telem_data[["Time","RPM","Speed","nGear","Throttle","Brake","X","Y","Z"]]

nec_data.to_csv("data.csv", index=False)

